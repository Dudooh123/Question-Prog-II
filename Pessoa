package treinando_for_prova;

public class Pessoa {
	public String nome;
	public String nacionalidade;
	
	
	public Pessoa(String nome, String nacionalidade) {
		super();
		this.nome = nome;
		this.nacionalidade = nacionalidade;
	}
	
	public String toString() {
		return "Nome " + nome + ", Nacionalidade: " + nacionalidade;
	}

}
